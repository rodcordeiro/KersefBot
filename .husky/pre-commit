# .husky/pre-commit
eslint $(git diff --cached --name-only --diff-filter=ACMR | grep -E '\.(ts)$' | sed 's| |\\ |g') --max-warnings 5  --fix  --ignore-pattern 'dist/*' 
prettier $(git diff --cached --name-only --diff-filter=ACMR | grep -E '\.(ts)$' | sed 's| |\\ |g') --write --ignore-unknown
git update-index --again
