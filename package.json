{
  "name": "nodejs-typescript",
  "version": "0.2.0",
  "description": "",
  "main": "index.ts",
  "scripts": {
    "start:prod": "tsc -P . && node dist/index.js",
    "start:dev": "ts-node --transpileOnly src/index.ts",
    "prebuild": "rimraf dist && npm run format",
    "build": "tsc -b .",
    "format": "npm run lint && npm run pretty",
    "pretty": "prettier --write .",
    "lint": "eslint --fix --ext .ts,.tsx,.js,.jsx ./src",
    "typeorm": "ts-node ./node_modules/typeorm/cli.js",
    "migration:generate": "npm run typeorm -- migration:generate ./src/migrations/ -d ./src/database/index.ts",
    "migration:run": "npm run typeorm -- migration:run -d ./src/database/index.ts",
    "prepare": "husky"
  },
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@discordjs/builders": "^1.13.0",
    "discord.js": "^14.24.1",
    "dotenv": "^17.2.3",
    "mysql2": "^3.15.3",
    "node-cron": "^4.2.1",
    "node-fetch": "^3.3.1",
    "pagination.djs": "^4.0.18",
    "reflect-metadata": "^0.2.2",
    "sharp": "^0.34.4",
    "tesseract.js": "^6.0.1",
    "ts-node": "^10.9.2",
    "typeorm": "^0.3.27",
    "typescript": "^5.9.3",
    "uuid": "^13.0.0"
  },
  "devDependencies": {
    "@eslint/js": "^9.38.0",
    "@types/node": "^20.19.24",
    "eslint": "^9.38.0",
    "globals": "^16.4.0",
    "husky": "^9.1.7",
    "jiti": "^2.6.1",
    "prettier": "^3.6.2",
    "rimraf": "^6.0.1",
    "tsx": "^4.20.6",
    "typescript-eslint": "^8.46.2"
  },
  "lint-staged": {
    "*": "your-cmd"
  }
}
